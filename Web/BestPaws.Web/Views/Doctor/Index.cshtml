@model BestPaws.Web.ViewModels.Doctor.DoctorListViewModel
@{
    this.ViewData["Title"] = "Our Team";
}
@*<h2 class="text-info">Our services</h2>*@
<header class="jumbotron my-3">
    <img class="img-fluid" src="~/img/image.jpg" alt="Our Team" />
    <h3 class="text-center">Our Team</h3>
</header>

<div class="d-flex flex-fill">
    <div class="row my-lg-5">
        @foreach (var doctor in this.Model.Doctors)
        {
            <div class="col-md-6">
                <div class="card m-2">
                    <div class="card-header">
                        <img class="card-img" src="assets/img/dog-1.jpg" alt="Our team">
                    </div>
                    <div class="card-body">
                        <h4 class="card-title mt-0">@doctor.FirstName @doctor.LastName</h4>
                        <p class="card-text">@doctor.Biography </p>
                    </div>
                    <div class="card-footer">
                        <a href="#a" class="btn btn-outline-info">Read More</a>
                    </div>
                </div>
            </div>
        }
    </div>
</div>
